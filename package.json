{
  "name": "pokemon-remix",
  "version": "1.0.0",
  "description": "A Pokemon illustrated book with Remix",
  "private": true,
  "sideEffects": false,
  "author": "Konippi",
  "type": "module",
  "packageManager": "pnpm@9.9.0",
  "scripts": {
    "build": "remix vite:build",
    "dev": "remix vite:dev",
    "start": "remix-serve ./build/server/index.js",
    "typecheck": "tsc",
    "format": "biome format .",
    "format:apply": "biome format . --write",
    "lint": "biome lint .",
    "lint:apply": "biome lint . --write",
    "lint:style": "stylelint src/**/*.css --aei",
    "lint:style:apply": "stylelint src/**/*.css --fix --aei",
    "check": "biome check .",
    "sb": "storybook dev",
    "build-sb": "storybook build"
  },
  "dependencies": {
    "@emotion/react": "11.13.3",
    "@emotion/styled": "11.13.0",
    "@mui/icons-material": "6.0.2",
    "@radix-ui/react-avatar": "1.1.0",
    "@radix-ui/react-scroll-area": "1.1.0",
    "@radix-ui/react-slot": "1.1.0",
    "@remix-run/node": "2.11.2",
    "@remix-run/react": "2.11.2",
    "@remix-run/serve": "2.11.2",
    "browserslist": "4.23.3",
    "class-variance-authority": "0.7.0",
    "clsx": "2.1.1",
    "isbot": "4.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwind-merge": "2.5.2"
  },
  "devDependencies": {
    "@biomejs/biome": "1.8.3",
    "@chromatic-com/storybook": "^1.9.0",
    "@remix-run/dev": "2.11.2",
    "@storybook/addon-essentials": "8.2.9",
    "@storybook/addon-interactions": "8.2.9",
    "@storybook/addon-links": "8.2.9",
    "@storybook/addon-onboarding": "8.2.9",
    "@storybook/addon-viewport": "8.2.9",
    "@storybook/blocks": "8.2.9",
    "@storybook/react": "8.2.9",
    "@storybook/react-vite": "8.2.9",
    "@storybook/test": "8.2.9",
    "@types/react": "18.2.20",
    "@types/react-dom": "18.2.7",
    "autoprefixer": "10.4.19",
    "cssnano": "7.0.6",
    "esbuild": "0.23.1",
    "postcss": "8.4.38",
    "postcss-nested": "6.2.0",
    "storybook": "8.2.9",
    "stylelint": "16.9.0",
    "stylelint-config-standard": "36.0.1",
    "tailwindcss": "3.4.4",
    "typescript": "5.1.6",
    "vite": "5.1.0",
    "vite-tsconfig-paths": "4.2.1"
  },
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9.0.0",
    "npm": "No use `npm`, use `pnpm`",
    "yarn": "No use `yarn`, use `pnpm`"
  },
  "volta": {
    "node": "22.8.0",
    "pnpm": "9.9.0"
  }
}
